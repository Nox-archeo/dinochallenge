# Configuration du Bot Telegram Dino Challenge
# Copiez ce fichier vers .env et remplissez les valeurs

# Token du bot Telegram (obtenu via @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Clés PayPal Developer (https://developer.paypal.com/)
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_SECRET_KEY=your_paypal_secret_key_here

# Mode PayPal (True pour sandbox/test, False pour production)
PAYPAL_SANDBOX=True

# URL du jeu Dino hébergé sur GitHub Pages
GAME_URL=https://your-username.github.io/dinochallenge/
